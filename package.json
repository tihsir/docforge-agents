{
  "name": "docforge-agents",
  "version": "0.1.0",
  "description": "CLI tool for generating senior-level planning artifacts through AI-assisted collaboration",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "docforge": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "documentation",
    "rfc",
    "planning",
    "ai",
    "agents",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.15",
    "openai": "^4.56.0",
    "ora": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
